{
  "name": "@marbletech/nestjs-prom",
  "version": "2.0.0",
  "description": "A promotheus module for nestjs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rm -rf dist && tsc -p tsconfig.json",
    "lint": "eslint './**/*.ts' --ignore-pattern \"dist/*\"",
    "lint:fix": "eslint './**/*.ts' --fix --ignore-pattern \"dist/*\"",
    "precommit": "lint-staged",
    "prepublish:npm": "npm run build",
    "publish:npm": "npm publish --access public",
    "prepublish:next": "npm run build",
    "publish:next": "npm publish --access public --tag next",
    "test:e2e": "jest --config ./tests/jest-e2e.json --runInBand",
    "test:e2e:dev": "jest --config ./tests/jest-e2e.json --runInBand --watch",
    "test:debug": "node --inspect-brk ./node_modules/.bin/ts-node tests/src/main.ts",
    "test:live": "ts-node tests/src/main.ts"
  },
  "author": "Tieu-Philippe KHIM <pkhim@digikare.com>",
  "license": "MIT",
  "devDependencies": {
    "@nestjs/common": "^8.0.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^12.12.53",
    "@types/response-time": "^2.3.4",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "eslint": "^8.57.0",
    "eslint-plugin-import": "^2.29.1",
    "jest": "^29.7.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "supertest": "^4.0.2",
    "ts-jest": "^29.1.2",
    "ts-node": "^8.3.0",
    "typescript": "^5.4.5"
  },
  "peerDependencies": {
    "@nestjs/common": "^6.0.0 || ^7.0.0 || ^8.0.0",
    "@nestjs/core": "^6.0.0 || ^7.0.0 || ^8.0.0",
    "prom-client": "^15.0.0",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.0.0 || ^7.0.0"
  },
  "dependencies": {
    "response-time": "^2.3.2",
    "url-value-parser": "^2.0.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/digikare/nestjs-prom.git"
  },
  "keywords": [
    "nestjs",
    "promotheus"
  ],
  "bugs": {
    "url": "https://github.com/digikare/nestjs-prom/issues"
  },
  "homepage": "https://github.com/digikare/nestjs-prom#readme",
  "contributors": [
    "Michael Yankelev @FSM1",
    "Ashleigh Simonelli @bashleigh",
    "@blackkopcap",
    "@litichevskiydv"
  ],
  "publishConfig": {
    "tag": "next"
  }
}
